/* pages/my/my.wxss */
page {
  background: #f5f6f7;
}

.login {
  width: 100%;
  height: 348rpx;
  background: #fff;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.login image {
  width: 180rpx;
  height: 180rpx;
  border-radius: 50%;
  margin-bottom: 25rpx;
}

/* 我的导航 */
.navs {
  width: 100%;
  height: 540rpx;
  background: #fff;
  margin-top: 20rpx;
}

.navs .nav {
  width: 100%;
  height: 92rpx;
  border-bottom: 2rpx solid #e5e5e5;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0rpx 24rpx;
  box-sizing: border-box;
}